<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="rk.device.launcher">

    <application
        android:name=".global.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.SettingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.SetTimeActivity">
        </activity>
        <activity android:name=".ui.activity.SetNetWorkActivity">
        </activity>
        <activity android:name=".ui.activity.SetDoorGuardActivity">
        </activity>
        <activity android:name=".ui.activity.SelectItemListActivity">
        </activity>
    </application>

</manifest>